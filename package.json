{
  "name": "lambda-boilerplate-node",
  "version": "0.0.1",
  "description": "Get started quick building and deploying a full Node.js application quick and painlessly on AWS Lambda",
  "author": "Jordan Ramos",
  "license": "ISC",
  "keywords": [
    "lambda",
    "aws",
    "node",
    "boilerplate",
    "api",
    "gateway"
  ],
  "homepage": "https://github.com/TimeInc/lambda-boilerplate-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeInc/lambda-boilerplate-node.git"
  },
  "bugs": {
    "url": "https://github.com/TimeInc/lambda-boilerplate-node/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "ENV=test nyc mocha --compilers js:babel-core/register -R dot -u bdd test",
    "test:watch": "ENV=test nyc mocha --compilers js:babel-core/register -R dot -u bdd -w test",
    "coverage": "ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint": "ENV=test eslint -c node_modules/tardis-common/.eslintrc src",
    "lint:test": "ENV=test eslint -c node_modules/tardis-common/.eslintrc test",
    "build": "rm -rf dist && mkdir dist && babel src -d dist",
    "bundle": "npm run build && cd dist && zip -rq bundle.zip . && mv bundle.zip ../",
    "start": "nodemon index.js --exec babel-node"
  },
  "dependencies": {
    "assign-deep": "^0.4.5",
    "caas-content-client-node": "^1.0.10",
    "moment": "^2.15.1",
    "striptags": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-latest": "6.14.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "eslint": "^3.7.1",
    "express": "^4.14.0",
    "mocha": "3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^8.3.1",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "tardis-common": "^1.8.11"
  }
}
