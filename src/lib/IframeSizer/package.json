{
  "name": "iframe-sizer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "test": "npm run test:run",
    "bundle": "npm run build && npm run bundle:zip",
    "build": "npm run build:clean && npm run build:compile && npm run build:install",
    "lint": "npm run lint:src && npm run lint:test",
    "watch": "npm run watch:test",
    "bundle:zip": "zip -rq bundle.zip dist",
    "watch:test": "ENV=test nyc mocha --compilers js:babel-core/register -R dot -u bdd -w test",
    "test:run": "ENV=test nyc mocha --compilers js:babel-core/register -R dot -u bdd test",
    "test:cover": "ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "lint:src": "ENV=test eslint -c node_modules/tardis-common/.eslintrc src",
    "lint:test": "ENV=test eslint -c node_modules/tardis-common/.eslintrc test",
    "build:compile": "babel src -d dist -D",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:install": "cp package.json dist && find dist -type f -name 'package.json' -execdir npm install --production {} \\;"
  },
  "author": "Jordan Ramos",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-copy-package-json": "^0.1.0",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-latest": "6.14.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "eslint": "^3.7.1",
    "express": "^4.14.0",
    "mocha": "3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^8.3.1",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "xmldom": "^0.1.27"
  }
}
